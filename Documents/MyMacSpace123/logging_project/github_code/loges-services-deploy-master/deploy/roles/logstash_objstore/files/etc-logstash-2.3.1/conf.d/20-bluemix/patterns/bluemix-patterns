IPLIST (?:.*,\s%{IP}|%{IP})
HTTPDATE2 %{MONTHDAY}/%{MONTHNUM}/%{YEAR}:%{TIME} %{INT}
ROUTERACCESSLOG %{IPORHOST:hostName} - \[%{HTTPDATE2:startTime}\] "%{WORD:method} %{NOTSPACE:requestURI} %{DATA:protocol}" %{NUMBER:statusCode} %{NUMBER:requestBytesReceived:int} (?:%{NUMBER:bodyBytesSent:int}|-) "%{DATA:referer}"(?:%{SPACE}|;%{SPACE})"%{DATA:userAgent}" %{HOSTPORT:remoteAddr} x_forwarded_for:"%{DATA:x_forwarded_for}"(?:%{SPACE}x_forwarded_proto:"%{DATA:x_forwarded_proto}"%{SPACE}|%{SPACE})vcap_request_id:%{DATA:vcap_request_id} response_time:(?:%{NUMBER:response_time:float}|-) app_id:(?:%{NOTSPACE:app_id}|)(?:%{SPACE}x_global_transaction_id:"%{DATA:x_global_transaction_id}"|%{SPACE})
NGINXACCESSLOG %{IPORHOST:hostName} - \[%{HTTPDATE:timestamp}\] "%{WORD:method} %{NOTSPACE:requestURI} %{DATA:protocol}" %{NUMBER:statusCode} (?:%{NUMBER:bodyBytesSent:int}|-) "%{DATA:referer}" "%{DATA:userAgent}" %{IPLIST:requestIplist} vcap_request_id:%{DATA:vcap_request_id} response_time:%{NUMBER:response_time:float}
