
LIMLOG_PREFIX %{MONTH:month} %{MONTHDAY:monthDay} %{TIME:time} %{YEAR:year} %{INT:procId} %{INT:logLevel} %{INT}\.%{INT}\.%{INT}

LIMLOG_METHOD %{WORD:methodName}(\(\))?\:
LIMLOG_HOST (H|h)ost <%{IPORHOST:hostName}>
LIMLOG_CLUSTER cluster <%{WORD:cluster}>

LIMLOG_GENERIC %{LIMLOG_PREFIX} (%{LIMLOG_METHOD})?%{GREEDYDATA}

LIMLOG_HOST_CLUSTER %{LIMLOG_PREFIX} (%{LIMLOG_METHOD})?%{GREEDYDATA}%{LIMLOG_HOST}%{GREEDYDATA}%{LIMLOG_CLUSTER}

LIMLOG %{LIMLOG_HOST_CLUSTER}|%{LIMLOG_GENERIC}
